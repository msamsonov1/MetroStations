WMATA_API_KEY = 'APIHERE' #from api.wmata.com register with your email

def get_train_positions():
    conn = http.client.HTTPSConnection('api.wmata.com')
    headers = {'api_key': WMATA_API_KEY}
    
    conn.request('GET', '/TrainPositions/TrainPositions?contentType=json', headers=headers)
    response = conn.getresponse()
    data = response.read().decode('utf-8')
    conn.close()
    return json.loads(data)

data = get_train_positions()
train_positions = data['TrainPositions']
x = []
y = []
labels = []

for train in train_positions:
    circuit_id = train['CircuitId']
    seconds_at_location = train.get('SecondsAtLocation')
    station_code = train.get('StationCode')
    #print(f"CircuitId: {circuit_id}, SecondsAtLocation: {seconds_at_location}")
    if circuit_id in [s['CircuitId'] for s in station_CircuitIds.values()]:
        station = next(s for s in station_CircuitIds.values() if s['CircuitId'] == circuit_id)
        x.append(station['lon'])
        y.append(station['lat'])
        labels.append(station['name'])
    
fig, ax = plt.subplots()
ax.scatter(x, y, marker='o', color='red', label='Train Stations')

for i, label in enumerate(labels):
    ax.annotate(label, (x[i], y[i]))

x.append(station_CircuitIds['C07']['lon'])
y.append(station_CircuitIds['C07']['lat'])
labels.append(station_CircuitIds['C07']['name'])
ax.scatter(station_CircuitIds['C07']['lon'], station_CircuitIds['C07']['lat'], marker='.', color='gray')
ax.annotate(station_CircuitIds['C07']['name'], (station_CircuitIds['C07']['lon'], station_CircuitIds['C07']['lat']))

